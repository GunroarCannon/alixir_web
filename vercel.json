{
    "version": 2,
    "public":true,
    "builds": [{
        "src": "./server.js",
        "use": "@vercel/node"
    },
    {
          "src": "/**",
          "use": "@vercel/static"
    }],
    "routes": [
        {
            "src": "/(.*)", 
            "dest": "server.js"
        },{
      "src": "/(.+\\.[a-z]+)$",
      "dest": "/$1"
        }
    ]
}
